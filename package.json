{
  "name": "tezbarakat-tj-frontend",
  "description": "TezBarakat TJ - Telegram Mini App Platform Frontend",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf node_modules/.vite-temp && NODE_OPTIONS=--max-old-space-size=4096 tsc -b && NODE_OPTIONS=--max-old-space-size=4096 vite build --mode production || exit 1",
    "build:prod": "rm -rf node_modules/.vite-temp && NODE_OPTIONS=--max-old-space-size=4096 tsc -b && NODE_OPTIONS=--max-old-space-size=4096 vite build --mode production || exit 1",
    "build:deploy": "npm run build && npm run deploy",
    "deploy": "rm -rf /var/www/tezbarakat-frontend/* && cp -r dist/* /var/www/tezbarakat-frontend/ && echo 'Deployed to Nginx directory successfully'",
    "build:check": "npm run lint && npm run type-check && npm run build",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "install-deps": "npm install",
    "clean": "rm -rf node_modules package-lock.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run lint && npm run type-check && npm test && npm run test:e2e",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.80.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query": "^5.90.12",
    "browser-image-compression": "^2.0.2",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.364.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.2.4",
    "react-router-dom": "^6.27.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "keywords": [
    "telegram",
    "mini-app",
    "lottery",
    "react",
    "vite",
    "supabase"
  ],
  "author": "TezBarakat Team",
  "homepage": "https://tezbarakat.com",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "happy-dom": "^20.0.10",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.12.0",
    "postcss": "8.4.49",
    "prettier": "^3.1.0",
    "supabase": "^2.72.1",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^7.2.2",
    "vite-plugin-source-identifier": "1.1.2",
    "vitest": "^4.0.7"
  }
}
