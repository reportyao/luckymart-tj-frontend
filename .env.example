# Supabase Configuration
# Use VITE_ prefix for client-side environment variables in Vite projects
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Telegram Mini App Configuration
# Direct Link from BotFather (without https://)
VITE_TELEGRAM_SHARE_LINK=t.me/your_bot_username/your_app_name

# App Environment
NODE_ENV=development

# Optional: Backend API Base URL (for future backend integration)
# VITE_API_BASE_URL=http://localhost:3000/api

# Optional: Google Maps API (if needed)
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
